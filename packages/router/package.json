{
  "name": "@theracode/router",
  "version": "0.0.8",
  "description": "Router for Stencil apps that focuses on smooth transitions and eliminating visual tearing",
  "module": "dist/collection/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/stencilrouter.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "deploy": "npm run build && np",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "echo 'someday'",
    "lint": "tslint --project .",
    "lint-fix": "tslint --project . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theracode/open-source.git"
  },
  "devDependencies": {
    "@stencil/core": "^0.9.2",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^20.0.8",
    "jest": "^22.4.3",
    "np": "^2.16.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.10.0",
    "@theracode/tslint-rules": "latest"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theracode/open-source"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*(test|spec)\\.(ts|tsx)$"
  },
  "homepage": "https://github.com/theracode/open-source"
}
